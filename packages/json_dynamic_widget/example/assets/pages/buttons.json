{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "Buttons"
          }
        }
      }
    },
    "body": {
      "type": "column",
      "args": {
        "children": [
          {
            "type": "expanded",
            "args": {
              "child": {
                "type": "center",
                "args": {
                  "child": {
                    "type": "elevated_button",
                    "args": {
                      "onLongPress": "${simplePrintMessage()}",
                      "style": {
                        "foregroundColor": {
                          "pressed": "FF2196F3",
                          "focused": "FF2196F3",
                          "empty": "FFF44336"
                        }
                      },
                      "child": {
                        "type": "text",
                        "args": {
                          "text": "ElevatedButton"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "type": "expanded",
            "args": {
              "child": {
                "type": "center",
                "args": {
                  "child": {
                    "type": "text_button",
                    "args": {
                      "onLongPress": "${simplePrintMessage()}",
                      "style": {
                        "foregroundColor": {
                          "pressed": "FF2196F3",
                          "focused": "FF2196F3",
                          "empty": "FFF44336"
                        }
                      },
                      "child": {
                        "type": "text",
                        "args": {
                          "text": "TextButton"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "type": "expanded",
            "args": {
              "child": {
                "type": "center",
                "args": {
                  "child": {
                    "type": "outlined_button",
                    "args": {
                      "onLongPress": "${simplePrintMessage()}",
                      "style": {
                        "foregroundColor": {
                          "pressed": "FF2196F3",
                          "focused": "FF2196F3",
                          "empty": "FFF44336"
                        }
                      },
                      "child": {
                        "type": "text",
                        "args": {
                          "text": "OutlinedButton"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "type": "expanded",
            "args": {
              "child": {
                "type": "center",
                "args": {
                  "child": {
                    "type": "icon_button",
                    "args": {
                      "icon": {
                        "type": "icon",
                        "args": {
                          "icon": {
                            "codePoint": 58712,
                            "fontFamily": "MaterialIcons"
                          }
                        }
                      },
                      "onPressed": "${simplePrintMessage()}"
                    }
                  }
                }
              }
            }
          }
        ]
      }
    },
    "floatingActionButton": {
      "type": "floating_action_button",
      "args": {
        "onPressed": "${simplePrintMessage()}",
        "child": {
          "type": "icon",
          "args": {
            "icon": {
              "codePoint": 57369,
              "fontFamily": "MaterialIcons"
            },
            "size": 50
          }
        }
      }
    }
  }
}